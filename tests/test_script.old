TEST_LINE="$MIDICVT -c tmp/ex1-mthd.asc -o tmp/ex1-mthd-recompiled.mid"
run_test tmp/ex1-mthd-recompiled.mid results/ex1-recompiled.mid

#-----------------------------------------------------------------------------
# midicvt, convert MIDI to MIDI file using --m2m option
